{
    "Version": "2012-10-17",
    "Id": "AWSLogDeliveryWrite20150319",
    "Statement": [
        {
            "Sid": "AWSLogDeliveryWrite",
            "Effect": "Allow",
            "Principal": {
                "Service": "delivery.logs.amazonaws.com"
            },
            "Action": "s3:PutObject",
            "Resource": [
                "arn:aws:s3:::kdc-test-bucket/AWSLogs/859234351948/*",
                "arn:aws:s3:::kdc-test-bucket/kyeongjin/AWSLogs/859234351948/*",
                "arn:aws:s3:::kdc-test-bucket/jinseok/AWSLogs/867099995276/*"
            ],
            "Condition": {
                "StringEquals": {
                    "s3:x-amz-acl": "bucket-owner-full-control",
                    "aws:SourceAccount": [
                        "859234351948",
                        "867099995276"
                    ]
                },
                "ArnLike": {
                    "aws:SourceArn": [
                        "arn:aws:logs:ap-northeast-2:859234351948:*",
                        "arn:aws:logs:ap-northeast-2:867099995276:*"
                    ]
                }
            }
        },
        {
            "Sid": "AWSLogDeliveryAclCheck",
            "Effect": "Allow",
            "Principal": {
                "Service": "delivery.logs.amazonaws.com"
            },
            "Action": "s3:GetBucketAcl",
            "Resource": [
                "arn:aws:s3:::kdc-test-bucket",
                "arn:aws:s3:::kdc-test-bucket/kyeongjin",
                "arn:aws:s3:::kdc-test-bucket/jinseok"
            ],
            "Condition": {
                "StringEquals": {
                    "aws:SourceAccount": [
                        "859234351948",
                        "867099995276"
                    ]
                },
                "ArnLike": {
                    "aws:SourceArn": [
                        "arn:aws:logs:ap-northeast-2:859234351948:*",
                        "arn:aws:logs:ap-northeast-2:867099995276:*"
                    ]
                }
            }
        }
    ]
}